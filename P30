#include <stdio.h>

int main() {
    int a, b, temp;

    printf("Enter the first number: ");
    scanf("%d", &a);

    printf("Enter the second number: ");
    scanf("%d", &b);

    
    temp = a;
    a = b;
    b = temp;

    printf("After swapping, the first number is: %d\n", a);
    printf("After swapping, the second number is: %d\n", b);

    return 0;
}
