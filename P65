#include <stdio.h>

int main() {
    int s1,s2,s3;
    printf("lengths of the three sides of the triangle: ");
    scanf("%d %d %d", &s1, &s2, &s3);
    if (s1 + s2 > s3 && s1 + s3 > s2 && s2 + s3 > s1) {
        printf("Triangle is formed .");
    } else {
        printf("Triangle cannot be formed.");
    }
 return 0;
}
