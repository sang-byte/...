#include <stdio.h>
 int main(){
    int inputInteger;
    char inputCharacter;
    float inputFloat;
    long int ilongint;
    unsigned long int iunsignedlongint;
    printf("Enter Integer,Character, Float number, long int and unsigned long int\n");
    scanf("%d %c %f  %ld %lu", &inputInteger, &inputCharacter,
        &inputFloat, &ilongint, &iunsignedlongint);
    printf("\nInteger you entered is : %d", inputInteger);
    printf("\nCharacter you entered is : %c", inputCharacter);
    printf("\nFloating point number you entered is : %f",
        inputFloat);
printf("\nlong int you entered is : %ld", ilongint);
printf("\nunsigned long int you entered is : %lu", iunsignedlongint);
    return 0;
}
