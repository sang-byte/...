#include <stdio.h>

int main() {
    float side, surfaceArea, volume;

    printf("Enter the side length of the cube: ");
    scanf("%f", &side);

    surfaceArea = 6 * side * side;
    volume = side * side * side;

    printf("Surface area of the cube: %.2f\n", surfaceArea);
    printf("Volume of the cube: %.2f\n", volume);

    return 0;
}
