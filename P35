#include <stdio.h>

int main() {
    int num1 = 10;
    int num2 = 5;
    num1 += num2;
    printf("Addition: %d\n", num1);
    num1 -= num2;
    printf("Subtraction: %d\n", num1);
    num1 *= num2;
    printf("Multiplication: %d\n",num1);
    num1 /= num2;
    printf("Division: %d\n",num1);
    return 0;
}
