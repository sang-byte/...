#include <stdio.h>
int main() {
    char character;
    printf("character: ");
    scanf("%c", &character);

    if ((character >= 'a' && character <= 'z') || (character >= 'A' && character <= 'Z')) {
        printf("Alphabet");
    } else if (character >= '0' && character <= '9') {
        printf("Digit");
    } else {
        printf("Special character");
    }
 return 0;
}
