#include <stdio.h>

int checkInteger30(int num1, int num2) {
    if (num1 == 30 || num2 == 30 || num1 + num2 == 30) {
        return 1;
    } else {
        return 0;
    }
}

int main() {
    int num1 = 10 , num2 = 20;

    int result = checkInteger30(num1, num2);

    if (result == 1) {
        printf("True\n");
    } else {
        printf("False\n");
    }

    return 0;
}
