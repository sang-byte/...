#include <stdio.h>

int main() {
    int a = 8;
    int b = 3;
    int c, d, e, f,g;

    c = a + b;
    d = a - b;
    e = a * b;
    f = a + b;
    g = a % b;

    printf("c: %d\n", c);
    printf("d: %d\n", d);
    printf("e: %d\n", e);
    printf("f: %d\n", f);
    printf("g: %d\n", g);

    return 0;
}
