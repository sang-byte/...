#include <stdio.h>
typedef enum {
  Monday,
  Tuesday,
  Wednesday,
  Thursday,
  Friday,
  Saturday,
  Sunday
} DaysOfWeek;

int main() {
  DaysOfWeek days[7] = {Monday, Tuesday, Wednesday, Thursday, Friday, Saturday, Sunday};

  for (int i = 0; i < 7; i++) {
    printf("Day %d: %d\n", i+1, days[i]);
  }

  return 0;
}
